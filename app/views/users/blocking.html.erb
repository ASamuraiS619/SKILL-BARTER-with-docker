<div class="container">
  <div class="row">
    <div class="col-md-3">
      <%= render 'layouts/global_navigation_side_bar' %>
    </div>
    <div class="col-md-9">
      <div class="row py-2 my-2">
        <div class="col-md-9">
          <h5><strong>ブロックしているユーザー</strong></h5>
        </div>
        <div class="row">
          <% @blocking.each do |blocking| %>
            <div class="col-md-6 my-3">
              <%= render 'users/skill_holder_card', user: blocking, follow: nil %>
            </div>
          <% end %>
        </div>
      </div>
      <%# ページネーション %>
      <div class="row">
        <div class="col-md-4 offset-md-4">
          <%= paginate @blocking %>
        </div>
      </div>
    </div>
  </div>
</div>
